{
  "name": "OpenSB",
  "tree": {
    "$className": "DataModel",
    "ServerScriptService": {
      "$properties": {
        "LoadStringEnabled": true
      },

      "sbActor": {
        "$className": "Actor",

        "sb": {
          "$path": "out/server/sb",
          "$properties": {
            "RunContext": "Server"
          },

          "clientScripts": {
            "$className": "ScreenGui",
            "$properties": {
              "ResetOnSpawn": false,
              "Enabled": false
            },

            "sbActor": {
              "$className": "Actor",

              "sb": {
                "$path": "out/client/sb",

                "start": {
                  "$path": "out/start.server.luau",
                  "$properties": {
                    "RunContext": "Client"
                  }
                }
              }
            },
            "sandbox": {
              "$path": "out/client/sandbox.luau",

              "start": {
                "$path": "out/start.server.luau",
                "$properties": {
                  "RunContext": "Client"
                }
              }
            }
          }
        }
      },
      "sandbox": {
        "$path": "out/server/sandbox.server.luau",
        "$properties": {
          "RunContext": "Server"
        }
      }
    },
    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "Players": {
      "$properties": {
        "CharacterAutoLoads": false
      }
    },
    "Lighting": {
      "$properties": {
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Future"
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "PlayerScriptsLoader": {
          "$className": "LocalScript"
        },
        "RbxCharacterSounds": {
          "$className": "LocalScript"
        },
        "PlayerModule": {
          "$className": "ModuleScript"
        }
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    },
    "Chat": {
      "ChatModules": {
        "$className": "Folder"
      },
      "ClientChatModules": {
        "$className": "Folder"
      },
      "ChatServiceRunner": {
        "$className": "Script"
      },
      "BubbleChat": {
        "$className": "LocalScript"
      },
      "ChatScript": {
        "$className": "LocalScript"
      }
    }
  }
}
