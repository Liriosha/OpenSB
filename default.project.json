{
  "name": "OpenSB",
  "tree": {
    "$className": "DataModel",

    "ServerScriptService": {
      "SB": {
        "$path": "src"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [0, 0, 0],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}
