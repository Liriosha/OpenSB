{
  "name": "OpenSB",
  "tree": {
    "$className": "DataModel",
    "ServerScriptService": {
      "sbActor": {
        "$className": "Actor",

        "sb": {
          "$path": "src/server/sb",

          "clientScripts": {
            "$className": "ScreenGui",
            "$properties": {
              "ResetOnSpawn": false,
              "Enabled": false
            },

            "sbActor": {
              "$className": "Actor",

              "sb": {
                "$path": "src/client/sb"
              }
            },

            "sandbox": {
              "$path": "src/client/sandbox.client.luau"
            }
          }
        }
      },

      "sandbox": {
        "$path": "src/server/sandbox.server.luau"
      }
    },
    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "Players": {
      "$properties": {
        "CharacterAutoLoads": false
      }
    },
    "Lighting": {
      "$properties": {
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Future"
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "PlayerScriptsLoader": {
          "$className": "LocalScript"
        },
        "RbxCharacterSounds": {
          "$className": "LocalScript"
        },
        "PlayerModule": {
          "$className": "ModuleScript"
        }
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    },
    "Chat": {
      "ChatModules": {
        "$className": "Folder"
      },
      "ClientChatModules": {
        "$className": "Folder"
      },
      "ChatServiceRunner": {
        "$className": "Script"
      },
      "BubbleChat": {
        "$className": "LocalScript"
      },
      "ChatScript": {
        "$className": "LocalScript"
      }
    }
  }
}
