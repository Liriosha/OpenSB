{
  "name": "OpenSB",
  "tree": {
    "$className": "DataModel",

    "ServerScriptService": {
      "SB": {
        "$path": "src/"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true,
        "SignalBehavior": "Immediate"
      }
    },

    "Players": {
      "$properties": {
        "CharacterAutoLoads": false
      }
    },

    "Lighting": {
      "$properties": {
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Future"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "PlayerScriptsLoader": {
          "$className": "LocalScript"
        },
        "RbxCharacterSounds": {
          "$className": "LocalScript"
        },
        "PlayerModule": {
          "$className": "ModuleScript"
        }
      }
    },

    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    },

    "Chat": {
      "ChatModules": {
        "$className": "Folder"
      },
      "ClientChatModules": {
        "$className": "Folder"
      },
      "ChatServiceRunner": {
        "$className": "Script"
      },
      "BubbleChat": {
        "$className": "LocalScript"
      },
      "ChatScript": {
        "$className": "LocalScript"
      }
    }
  }
}
